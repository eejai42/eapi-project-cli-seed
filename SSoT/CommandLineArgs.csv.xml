<CommandLineArgs>
  <CommandLineArg>
    <Name>authenticate</Name>
    <Description>Authenticate as a user</Description>
    <Aliases>auth</Aliases>
    <DataType>string</DataType>
    <IsOptional>TRUE</IsOptional>
    <IsList>FALSE</IsList>
    <ExampleValue>test@test.com</ExampleValue>
    <SortOrder>1</SortOrder>
  </CommandLineArg>
  <CommandLineArg>
    <Name>demoPassword</Name>
    <Description>the user's password</Description>
    <Aliases>p</Aliases>
    <DataType>string</DataType>
    <IsOptional>TRUE</IsOptional>
    <IsList>FALSE</IsList>
    <ExampleValue>pwd123</ExampleValue>
    <SortOrder>3</SortOrder>
  </CommandLineArg>
  <CommandLineArg>
    <Name>invoke</Name>
    <Description>Invoke a method</Description>
    <Aliases>i</Aliases>
    <DataType>string</DataType>
    <IsOptional>TRUE</IsOptional>
    <IsList>FALSE</IsList>
    <ExampleValue>AddCell</ExampleValue>
    <SortOrder>4</SortOrder>
  </CommandLineArg>
  <CommandLineArg>
    <Name>bodyData</Name>
    <Description>Raw data provided</Description>
    <Aliases>d</Aliases>
    <DataType>string</DataType>
    <IsOptional>TRUE</IsOptional>
    <IsList>FALSE</IsList>
    <ExampleValue>{'cellâ€¦}</ExampleValue>
    <SortOrder>5</SortOrder>
  </CommandLineArg>
  <CommandLineArg>
    <Name>bodyFile</Name>
    <Description>Path to file to use</Description>
    <Aliases>f</Aliases>
    <DataType>string</DataType>
    <IsOptional>TRUE</IsOptional>
    <IsList>FALSE</IsList>
    <ExampleValue>Cell.json</ExampleValue>
    <SortOrder>6</SortOrder>
  </CommandLineArg>
  <CommandLineArg>
    <Name>runas</Name>
    <Description>Who to run as</Description>
    <Aliases>as</Aliases>
    <DataType>string</DataType>
    <IsOptional>FALSE</IsOptional>
    <IsList>FALSE</IsList>
    <DefaultValue>Guest</DefaultValue>
    <ExampleValue>Admin</ExampleValue>
    <SortOrder>7</SortOrder>
  </CommandLineArg>
  <CommandLineArg>
    <Name>amqps</Name>
    <Description>AMQPS Connection String</Description>
    <Aliases>amqp</Aliases>
    <DataType>string</DataType>
    <IsOptional>FALSE</IsOptional>
    <IsList>FALSE</IsList>
    <DefaultValue>amqps://smqPublic:smqPublic@effortlessapi-rmq.ssot.me/ej-tictactoe-demo</DefaultValue>
    <ExampleValue>amqps://smqPublic:smqPublic@effortlessapi-rmq.ssot.me/ej-tictactoe-demo</ExampleValue>
  </CommandLineArg>
</CommandLineArgs>